  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:a": "cross-env ANALYZER=true node scripts/build.js",
    "build:m:d": "rimraf build/main && tsc --project tsconfig.main.dev.json",
    "build:m:p": "rimraf build/main && tsc --project tsconfig.main.prod.json",
    "build:r:d": "rimraf build/renderer && yarn build && ncp index.html build/renderer/index.html",
    "build:r:p": "rimraf build/renderer && cross-env GENERATE_SOURCEMAP=false yarn build:r:d",
    "test": "node scripts/test.js",
    "dev:m2": "yarn set-debug-ns && node ./.electron/start-main.js",
    "dev:r": "cross-env COLOR=1 node ./.electron/start-renderer.js",
    "dev:rw": "cross-env COLOR=1 node ./.electron/start-renderer.js | cat",
    "dev:r:d": "node --inspect-brk ./.electron/start-renderer.js",
    "dev:m": "yarn build:m:d --watch",
    "start:m": "cross-env DEBUG=main:* node ./node_modules/electron/cli.js ./build/main/main/index.dev.js",
    "dbg:m": "cross-env DEBUG=main:* node ./node_modules/electron/cli.js ./build/main/main/index.dev.js --inspect-brk",
    "set-debug-ns": "cross-env DEBUG=main:*",
    "//dev": "concurrently 'yarn dev:r'  'yarn dev:m' 'yarn start:m' ",
    "//dbg": "yarn set-debug-ns && node .electron/dev-runner.js --dbg",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix 'src/**/*.ts{,x}' -e '**/*.test.ts'",
    "package": "electron-builder build --publish never",
    "pack:m": "node ./.electron/build.js",
    "dist": "yarn build:r:p && yarn build:m:p && yarn package",
    "distme": "yarn build:r:d && yarn build:m:d && yarn package",
    "run:mac": "./dist/mac/BlinkMind.app/Contents/MacOS/BlinkMind"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "main": "./build/main/main/index.js",
  "//build": {
    "productName": "BlinkMind",
    "appId": "com.github.awehook.blinkminddesktop",
    "fileAssociations": [
      {
        "ext": [
          "bmind"
        ],
        "name": "BlinkMind",
        "description": "Blink Mind Document",
        "role": "Editor",
        "icon": "icons/icon.icns"
      }
    ],
    "files": [
      "build/main/**/*",
      "build/renderer/**/*",
      "index.html",
      "icons/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "nsis": {
      "artifactName": "${productName}-${version}.${ext}",
      "oneClick": false,
      "perMachine": true,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "icon": "icons/icon.png",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "icon": "icons/icon.png",
      "target": [
        "nsis",
        "msi"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "snap",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "output": "dist"
    }
  }
}
